{
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "root": true,
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
        "vue"
    ],
    "settings": {
        "html/html-extensions": [
            ".html",
            ".vue"
        ]
    },
    "rules": {
        "no-console": 1,
        "no-unused-vars": 1,
        "no-undef": 1,
        "no-empty": 1,
        "vue/require-v-for-key": 1,
        "vue/valid-v-for": 1,
        "vue/no-parsing-error": [
            1,
            {
                "x-invalid-end-tag": false
            }
        ],
        "vue/no-unused-vars": 1
    }
}